            test = "not(.//db:example)"
            "example must not occur among the children or descendants of table"
          ]
        ]
      ]
      s:pattern [
        s:title [ "Element exclusion" ]
        s:rule [
          context = "db:table"
          s:assert [
            test = "not(.//db:figure)"
