<article xmlns="http://docbook.org/ns/docbook" version="5.0">
<title>Article wrapper</title>

  <section>
    <title>imagedata/@fileref + numbering + sep.</title>
    <programlistingco>
      <areaspec>
        <area coords="1" xml:id="prologue1"/>
        <area coords="4" xml:id="skipeof1"/>
        <areaset xml:id="xreq1">
          <area coords="9" xml:id="require11"/>
          <area coords="10" xml:id="require1"/>
        </areaset>
        <area coords="11 12" xml:id="use1"/>
        <area coords="27" xml:id="funccall1"/>
      </areaspec>
      <programlisting linenumbering="numbered"
          ><?dbhtml linenumbering.everyNth="5" linenumbering.separator=" >"
          ?><?dbfo linenumbering.everyNth="5" linenumbering.separator=" >"
          ?><inlinemediaobject>
      <imageobject>
        <imagedata format="linespecific"
                   fileref="codesample.txt"/>
      </imageobject>
      </inlinemediaobject></programlisting>
    </programlistingco>
  </section>

  <section>
    <title>inlinegraphic/@fileref + numbering</title>
    <programlistingco>
      <areaspec>
        <area coords="1" xml:id="prologue2"/>
        <area coords="4" xml:id="skipeof2"/>
        <areaset xml:id="xreq2">
          <area coords="9" xml:id="require12"/>
          <area coords="10" xml:id="require22"/>
        </areaset>
        <area coords="11 12" xml:id="use2"/>
        <area coords="27" xml:id="funccall2"/>
      </areaspec>
      <programlisting linenumbering="numbered"><inlinemediaobject>
      <imageobject>
      <imagedata format="linespecific"
                 fileref="codesample.txt"/>
      </imageobject></inlinemediaobject></programlisting>
    </programlistingco>
  </section>

  <section>
    <title>textdata/@fileref w/o numbering or sep</title>
    <programlistingco>
      <areaspec>
        <area coords="1" xml:id="prologue3"/>
        <area coords="4" xml:id="skipeof3"/>
        <areaset xml:id="xreq3">
          <area coords="9" xml:id="require13"/>
          <area coords="10" xml:id="require3"/>
        </areaset>
        <area coords="11 12" xml:id="use3"/>
        <area coords="27" xml:id="funccall3"/>
      </areaspec>
      <programlisting linenumbering="unnumbered"><textobject>
      <textdata format="linespecific"
                fileref="codesample.txt"/>
      </textobject></programlisting>
    </programlistingco>
  </section>

</article>
